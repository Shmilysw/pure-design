AAA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,SAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OARA;AASA,KA9EA;AA+EA,IAAA,KA/EA,mBA+EA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAlFA;AAmFA,IAAA,gBAnFA,4BAmFA,QAnFA,EAmFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KAvFA;AAwFA,IAAA,mBAxFA,+BAwFA,OAxFA,EAwFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA5FA;AA6FA,IAAA,QA7FA,oBA6FA,GA7FA,EA6FA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA;AA/FA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\" v-if=\"user.role === 'ROLE_ADMIN'\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"\r\n              @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"数据库ID\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"数据库名\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"数据库表数量\"></el-table-column>\r\n      <el-table-column prop=\"times\" label=\"数据库描述\"></el-table-column>\r\n      <el-table-column prop=\"teacher\" label=\"数据库类型\"></el-table-column>\r\n      <el-table-column label=\"启用\">\r\n        <template slot-scope=\"scope\">\r\n          <el-switch v-model=\"scope.row.state\" active-color=\"#13ce66\" inactive-color=\"#ccc\"\r\n                     @change=\"changeEnable(scope.row)\"></el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"280\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n<!--          <el-button type=\"primary\" @click=\"selectCourse(scope.row.id)\"></el-button>-->\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\" v-if=\"user.role === 'ROLE_ADMIN'\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" v-if=\"user.role === 'ROLE_ADMIN'\">删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"padding: 10px 0;display: flex;justify-content: center;\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"课程信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\r\n      <el-form label-width=\"100px\" size=\"small\">\r\n        <el-form-item label=\"数据库名\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"数据库表数量\">\r\n          <el-input v-model=\"form.score\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"数据库描述\">\r\n          <el-input v-model=\"form.times\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"数据库类型\">\r\n          <el-select clearable v-model=\"form.teacherId\" placeholder=\"请选择\">\r\n            <el-option v-for=\"item in teachers\" :key=\"item.id\" :label=\"item.nickname\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"Course\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      tableData: [],\r\n      name: '',\r\n      multipleSelection: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogFormVisible: false,\r\n      teachers: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    selectCourse(courseId) {\r\n      this.request.post('/course/studentCourse/' + courseId + \"/\" + this.user.id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"选课成功\")\r\n        } else {\r\n          this.$message.success(res.msg)\r\n        }\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/course/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n\r\n      })\r\n\r\n      this.request.get(\"/user/role/ROLE_TEACHER\").then(res => {\r\n        this.teachers = res.data\r\n      })\r\n    },\r\n    changeEnable(row) {\r\n      this.request.post(\"/course/update\", row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/course/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/course/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/course\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}